{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "baseUrl": ".",
    "paths": {
      "@modelcontextprotocol/sdk/dist/esm/types": ["node_modules/@modelcontextprotocol/sdk/dist/esm/types.js"],
      "@modelcontextprotocol/sdk/dist/esm/shared/protocol": ["node_modules/@modelcontextprotocol/sdk/dist/esm/shared/protocol.js"]
    },
    "typeRoots": ["./types", "./node_modules/@types"],
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src"
  },
  "include": ["src", "types"],
  "exclude": ["dist", "node_modules"]
}
